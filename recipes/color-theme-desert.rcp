(:name color-theme-desert
       :description "A port of VIM's desert color theme to Emacs."
       :type git
       :url "https://github.com/superbobry/color-theme-desert.git"
       :depends color-theme
       :post-init (lambda ()
                    (autoload 'color-theme-desert "color-theme-desert"
                      "color-theme: desert" t)))
