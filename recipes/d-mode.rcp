(:name d-mode
       :description "Digitalmars-D-Mode for emacs"
       :type http
       :url "http://www.billbaxter.com/etc/d-mode.el"
       :post-init (lambda ()
		    (add-to-list 'auto-mode-alist '("\\.d[i]?$" . d-mode))
		    (autoload 'd-mode "d-mode" "Major mode for editing D code." t)
		    )
       )
