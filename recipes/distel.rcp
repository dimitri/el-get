(:name distel
       :website "https://github.com/massemanet/distel"
       :description "Distributed Emacs Lisp for Erlang."
       :type github
       :pkgname "massemanet/distel"
       :info "doc"
       :build `,(mapcar
                 (lambda (target)
                   (concat "make " target " EMACS=" el-get-emacs))
                 '("clean" "all"))
       :load-path ("elisp")
       :features distel)

