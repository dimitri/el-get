(:name fsharp-mode
       :description "Major mode for editing fsharp code."
       :type svn
       :url "https://fsharp-mode.svn.sourceforge.net/svnroot/fsharp-mode"
       :load-path (".")
       :features (fsharp inf-fsharp)
       :post-init (add-to-list 'auto-mode-alist
                               '("\\.fs[iylx]?$" . fsharp-mode)))
