(:name hideshow-org
       :description "Provides org-mode like hide and show for hideshow.el"
       :type github
       :pkgname "shanecelis/hideshow-org"
       :prepare (progn
                  (autoload 'hs-org/minor-mode "hideshow-org" nil t)
                  (defun hs-org-minor-mode-on ()
                    "Turn on `hs-org/minor-mode' mode."
                    (interactive)
                    (hs-org/minor-mode 1))))
