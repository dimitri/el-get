(:name mailcrypt
       :description "Mail encryption with PGP"
       :type cvs
       :url ":pserver:anonymous@mailcrypt.cvs.sourceforge.net:/cvsroot/mailcrypt"
       :module "mailcrypt"
       :build `(
                "autoconf"
                "./configure"
                ("make"
                 ,(concat "EMACS=" el-get-emacs)
                 "INFOFILES=mailcrypt.info"
                 "all"
                 "info")
                )
       :info  "."
       )
