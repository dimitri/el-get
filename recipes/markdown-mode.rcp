(:name markdown-mode
       :description "Major mode to edit Markdown files in Emacs"
       :type http
       :url "http://jblevins.org/git/markdown-mode.git/plain/markdown-mode.el"
       :before (add-to-list 'auto-mode-alist
                            '("\\.\\(md\\|mdown\\|markdown\\)\\'" . markdown-mode)))
