(:name mustache-mode
       :description "Emacs Major Mode for Mustache"
       :features mustache-mode
       :type github
       :pkgname "mustache/emacs"
       :prepare (progn
                  (add-to-list 'auto-mode-alist
                               '("\\.hs$" . mustache-mode))
                  (add-to-list 'auto-mode-alist
                               '("\\.handlebars$" . mustache-mode))))
