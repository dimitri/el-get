(:name n3-mode
       :description "Mode for Notation 3"
       :type github
       :pkgname "kurtjx/n3-mode-for-emacs"
       :prepare (progn
                  (autoload 'n3-mode "n3-mode"
                    "Major mode for OWL or N3 files" t)
                  (add-to-list 'auto-mode-alist
                               '("\\.\\(n3\\|owl\\)\\'" . n3-mode)))
       :post-init (add-hook 'n3-mode-hook 'turn-on-font-lock))
