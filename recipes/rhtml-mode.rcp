(:name rhtml-mode
       :description "Major mode for editing RHTML files"
       :type github
       :username "eschulte"
       :reponame "rhtml"
       :post-init (lambda ()
            (autoload 'rhtml-mode "rhtml-mode" nil t)
            (add-to-list 'auto-mode-alist '("\\.html\.erb$" . rhtml-mode))))
