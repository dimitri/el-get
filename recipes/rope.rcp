(:name rope
       :description "A python refactoring library"
       :post-init
       (lambda ()
         (unless (boundp 'pymacs-load-path) (setq pymacs-load-path nil))
         (add-to-list 'pymacs-load-path (el-get-package-directory "rope")))
       :type hg
       :url "http://bitbucket.org/agr/rope")
